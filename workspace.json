        "build": {
          "executor": "@nxext/stencil:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "projectType": "application",
            "tsConfig": "apps/doc/tsconfig.app.json",
            "configPath": "apps/doc/stencil.config.ts",
            "outputPath": "apps/doc"
          },
          "configurations": {
            "production": {
              "dev": false,
              "prod": true
            }
