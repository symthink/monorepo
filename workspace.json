{
  "version": 2,
  "projects": {
    "doc": {
      "root": "apps/doc",
      "sourceRoot": "apps/doc/src",
      "projectType": "application",
      "generators": {
        "@nxext/stencil:component": {
          "style": "scss"
        }
      },
      "tags": [],
      "targets": {
        "build": {
          "executor": "@nxext/stencil:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "projectType": "application",
            "tsConfig": "apps/doc/tsconfig.app.json",
            "configPath": "apps/doc/stencil.config.ts",
            "outputPath": "apps/doc"
          },
          "configurations": {
            "production": {
              "dev": false,
              "prod": true
            }
          }
        },
        "serve": {
          "executor": "@nxext/stencil:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "projectType": "application",
            "tsConfig": "apps/doc/tsconfig.app.json",
            "configPath": "apps/doc/stencil.config.ts",
            "outputPath": "apps/doc",
            "serve": true,
            "watch": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/doc/**/*.{ts,tsx}"
            ]
          }
        }
      }
    },
